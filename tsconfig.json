{
  "compilerOptions": {
    "module": "es2015",
    "target": "es5",
    "declaration": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "lib": [
      "dom",
      "es2015",
      "es2016",
      "es2017"
    ]
  },
  "exclude": [
    "dist",
    "node_modules",
    "autotest-example",
    "__jest__"
  ]
}
